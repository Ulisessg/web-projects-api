{
  "version": 2,
  "builds": [
    {
      "src": "/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "headers": {
        "Content-Type": "application/json",
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "/api/index.js"
    }
  ],
  "env": {
    "URI": "@uri",
    "PORT": "@port"
  }
}